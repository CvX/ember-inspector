<div class="toolbar">
  {{ui/toolbar-clear-button
    action=(action send "clearProfiles")
    classNames="toolbar__icon-button"
  }}

  {{#unless showEmpty}}
    <Ui::ToolbarDivider />
    {{ui/toolbar-reload-button
      action=(action send "refreshPage")
      classNames="toolbar__icon-button js-toolbar-page-refresh-btn"
    }}
  {{/unless}}

  <Ui::ToolbarDivider />

  <div class="toolbar__search js-render-profiles-search">
    {{ui/toolbar-search-field value=searchValue}}
  </div>
</div>
